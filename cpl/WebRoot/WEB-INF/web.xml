<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
  <servlet>
    <servlet-name>MyServlet3</servlet-name>
    <display-name>This is the display name of my J2EE component</display-name>
    <description>This is the description of my J2EE component</description>
    <servlet-class>com.cpl.servlet.MyServlet3</servlet-class>
    <init-param>
       <param-name>encoding</param-name>
       <param-value>utf-8</param-value>
    </init-param>
  </servlet>
  <servlet>
    <servlet-name>MyInitServlet</servlet-name>
    <display-name>This is the display name of my J2EE component</display-name>
    <description>This is the description of my J2EE component</description>
    <servlet-class>com.cpl.servlet.MyInitServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>


  <servlet-mapping>
    <servlet-name>MyServlet3</servlet-name>
    <url-pattern>/servlet/MyServlet3</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MyInitServlet</servlet-name>
    <url-pattern>/servlet/MyInitServlet</url-pattern>
  </servlet-mapping>
 
  

  <welcome-file-list>
     <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
</web-app>